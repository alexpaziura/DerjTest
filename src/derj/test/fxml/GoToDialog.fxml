<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="170.0" prefWidth="241.0" styleClass="toolbars" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="derj.test.code.GoToDialogController">
   <children>
      <VBox>
         <children>
            <ComboBox fx:id="cbRoz" prefHeight="25.0" prefWidth="242.0" promptText="Виберіть розділ" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </ComboBox>
            <TextField fx:id="tfQuest" promptText="Введіть питання">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </VBox>
      <HBox alignment="TOP_CENTER">
         <children>
            <Button id="btnAccept" fx:id="btnAccept" mnemonicParsing="false" onAction="#handleOk" onMousePressed="#clickAccept" onMouseReleased="#offclickAccept" prefHeight="50.0" prefWidth="50.0" styleClass="buttons">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button id="btnCancel" fx:id="btnCancel" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#handleCancel" onMousePressed="#clickCancel" onMouseReleased="#offclickCancel" prefHeight="50.0" prefWidth="50.0" styleClass="buttons">
               <font>
                  <Font name="Times New Roman" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="20.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
